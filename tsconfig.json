{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "downlevelIteration": true, // Needed to allow iteration over some objects like Map() while target is es5
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
  }
}
